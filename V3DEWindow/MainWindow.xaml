        xmlns:local="clr-namespace:V3DEWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <Style x:Key="WindowMainStyle1" TargetType="{x:Type Window}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Window}">
                        <ContentPresenter/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Grid>

        <Label Content="TEXT ASUME THERES STUFF HERE" HorizontalAlignment="Left" Margin="258,199,0,0" VerticalAlignment="Top"/>


    </Grid>
